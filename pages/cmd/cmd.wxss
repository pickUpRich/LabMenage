/* pages/cmd/cmd.wxss */
.foot-nav {
  height: 46px;
}

.foot-nav .con {
  position: fixed;
  height: 46px;
  left: 0;
  bottom: 0;
  right: 0;
  background: #fff;
  z-index: 999;
}

.fast-links {
  width: 160px;
  line-height: 18px;
  text-align: center;
}

.fast-links .iconfont {
  padding: 5px 0;
}

.fast-links .iconfont::before {
  display: block;
  margin-top: 5px;
  font-size: 18px;
}

.notempty {
  position: relative;
}

.notempty::after {
  content: "";
  display: block;
  width: 9px;
  height: 9px;
  border: 1px solid #fff;
  border-radius: 50%;
  background: #306fb4;
  position: absolute;
  top: 5px;
  right: 50%;
  margin-right: -16px;
}

.add-cart .add-cart-btn {
  background-color: rgb(48, 111, 180);
  color: #fff;
  border-radius: 50px;
  padding: 0px;
  width: 250px;
  height: 35px;
  line-height: 35px;
  margin-top: -10px;
}
